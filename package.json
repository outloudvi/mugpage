{
  "name": "mugpage",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "pre-edit": "npm run schema:generate",
    "schema:generate": "mkdir -p .generated && typescript-json-schema types.ts GameMeta > .generated/meta.schema.json",
    "schema:check": "npm run schema:generate && node scripts/validate.js"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@nuxt/content": "^1.11.1",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.17.2",
    "nuxt": "^2.15.6",
    "nuxt-buefy": "^0.4.10",
    "nuxt-fontawesome": "^0.4.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "ajv": "^8.6.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "fibers": "^5.0.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.2",
    "rehype-accessible-emojis": "^0.3.2",
    "remark-emoji": "^3.0.1",
    "remark-gfm": "^2.0.0",
    "sass": "^1.39.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript-json-schema": "^0.50.1",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2"
  },
  "repository": {
    "url": "https://github.com/outloudvi/mugpage"
  },
  "license": "MIT"
}
